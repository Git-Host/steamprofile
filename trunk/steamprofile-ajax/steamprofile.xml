<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<steamprofile-config>
	<theme>steamcom</theme>
	<templates>
		<profile>
		<![CDATA[
			<div>
				<div class="sp-avatar">
					<a rel="external" href="">
						<img src="" width="32" height="32" alt="" />
					</a>
				</div>
				<div class="sp-info">
					<a rel="external" href=""></a><br />
				</div>
				<div class="sp-extra">
					<div class="sp-handle"></div>
					<div class="sp-content">
						<a class="sp-joingame" title="Join Game" href=""><img src="" width="16" height="16" alt="" /></a>
						<a class="sp-addfriend" title="Add to Friends" href=""><img src="" width="16" height="16" alt="" /></a>
						<a class="sp-viewitems" title="View Items" href="" rel="external"><img src="" width="16" height="16" alt="" /></a>
					</div>
				</div>
			</div>
		]]>
		</profile>
		<loading>
		<![CDATA[
			<div>
				<div class="sp-loading">
					<img src="" width="32" height="32" alt="" />
					Loading...
				</div>
			</div>
		]]>
		</loading>
		<error>
		<![CDATA[
			<div>
				<div class="sp-error">
					<img src="" width="16" height="16" alt="Error" />
				</div>
			</div>
		]]>
		</error>
	</templates>
</steamprofile-config>